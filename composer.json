{
  "name": "nemmo/yii2-attachments",
  "description": "Extension for file uploading and attaching to the models",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "attachment",
    "file",
    "upload"
  ],
  "license": "BSD-4-Clause",
  "homepage": "https://github.com/Nemmo/yii2-attachments",
  "authors": [
    {
      "name": "Alimzhan Abuov aka Nemmo",
      "email": "alimzhan.abu@yandex.kz",
      "homepage": "http://alimzhan.kz",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.0",
    "kartik-v/yii2-widget-fileinput": "~1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpunit/dbunit": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "nemmo\\attachments\\": "src"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
